{
  "name": "@renegade-fanclub/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "init": "cp .dev.vars.example .dev.vars && bun run init:local",
    "check": "tsc && supabase db push --dry-run",
    "deploy": "supabase db push && supabase functions deploy",
    "dev": "supabase start",
    "types": "supabase gen types typescript --project-id rngfanclub-dev",
    "db:migrate:create": "supabase migration new rngfanclub-dev",
    "db:migrate:list": "supabase migration list",
    "db:migrate:apply": "supabase db push",
    "db:seed:local": "supabase db reset --file=seed.sql",
    "db:seed:remote": "supabase db reset --file=seed.sql",
    "init:local": "bun run db:clean && bun run db:deploy:local && bun run db:seed:local",
    "db:clean": "rm -rf .supabase/state",
    "db:deploy:local": "supabase db reset --file=schema.sql",
    "db:deploy:remote": "supabase db reset --file=schema.sql"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.5.2",
    "@cloudflare/workers-types": "^4.20241230.0",
    "@renegade-fanclub/types": "workspace:*",
    "typescript": "^5.6.3",
    "wrangler": "^3.97.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "jose": "^5.9.6",
    "supabase": "^2.12.1"
  }
}
